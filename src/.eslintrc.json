{
	"env": {
		"browser": false
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 3,
		"sourceType": "script"
	},
	"rules": {
		"indent": [ 0, "tab", { "ignoreComments": true } ],
		"linebreak-style": [ 2, "windows" ],
		"quotes": [ 1, "single" ],
		"semi": [ 2, "always" ],
		"no-unused-vars": [ 2, { "vars": "all", "args": "after-used" } ],
		"no-console": [ 2 ],
		"require-atomic-updates": [ 0 ]
	},
	"globals": {
		"app": true,

		"Project": true,
		"CompItem": true,
		"FolderItem": true,

		"TextDocument": true,
		"ImportOptions": true,

		"File": true,

		"D": true,
		"P": true,
		"F": true,
		"C": true,
		"E": true,
		"L": true,
		"T": true
	}
}